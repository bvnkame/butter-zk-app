{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "ts-watch": "tsc --noEmit --incremental --watch",
    "clear-cache": "npx rimraf public/cache && npx rimraf app/cache.json && echo \"UI Cache cleared successfully!\"",
    "deploy": "next build --no-lint && touch out/.nojekyll && node ./ghp-postbuild && git add -f out && git commit -m \"Deploy gh-pages\" && cd .. && git subtree push --prefix ui/out origin gh-pages"
  },
  "dependencies": {
    "coi-serviceworker": "^0.1.7",
    "comlink": "^4.4.2",
    "firebase": "^12.5.0",
    "jsonwebtoken": "8.5.1",
    "next": "14.2.12",
    "nostr-tools": "^2.17.2",
    "o1js": "^2.*",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "type": "module"
}
